programa {
  funcao inicio() {
    // comparando o uso de enquanto e faça - enquanto 
    logico condicao 

    condicao = falso
    enquanto (condicao) {
      escreva ("Mensagem dentro do faça-enquanto\n")
    }
    escreva ("Fora do enquanto\n")

    condicao = falso

    faca {
      escreva ("Mensagem dento do enquanto\n")
    } enquanto (condicao) 
    escreva ("Fora do faça-enquanto\n")
    }
  }

